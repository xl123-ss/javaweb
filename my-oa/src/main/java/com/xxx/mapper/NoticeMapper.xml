<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxx.mapper.NoticeMapper">
    <insert id="insert" parameterType="Notice"  useGeneratedKeys="true" keyColumn="notice_id" keyProperty="noticeId">
        INSERT INTO `oa`.`sys_notice` (`notice_id`, `receiver_id`, `content`, `create_time`)
        VALUES (null, #{receiverId}, #{content}, #{createTime});
    </insert>
</mapper>
